{
  "name": "noflo-webaudio",
  "description": "NoFlo components for Web Audio API.",
  "author": "Vilson Vieira <vilson@void.cc>",
  "repo": "automata/noflo-webaudio",
  "version": "0.2.0",
  "keywords": ["noflo", "audio", "webaudio", "webaudioapi"],
  "dependencies": {
    "noflo/noflo": "*"
  },
  "scripts": [
    "index.js",
    "vendor/tuna.js",
    "lib/Primative.coffee",
    "components/Play.coffee",
    "components/Oscillator.coffee",
    "components/Gain.coffee",
    "components/Chorus.coffee",
    "components/Convolver.coffee",
    "components/Panner.coffee",
    "components/AudioFile.coffee"
  ],
  "json": [
    "component.json"
  ],
  "noflo": {
    "icon": "music",
    "components": {
      "Play": "components/Play.coffee",
      "Oscillator": "components/Oscillator.coffee",
      "Gain": "components/Gain.coffee",
      "Chorus": "components/Chorus.coffee",
      "Convolver": "components/Convolver.coffee",
      "Panner": "components/Panner.coffee",
      "AudioFile": "components/AudioFile.coffee"
